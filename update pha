SELECT PHARM_CLM_ID,
       TRANS_TYP_CD,
       FIL_STS_CD_DESC,
       GDCB,
       GDCA
FROM (
    SELECT t.*,
           MAX(CASE WHEN TRANS_TYP_CD IS NOT NULL THEN 1 ELSE 0 END)
               OVER (PARTITION BY PHARM_CLM_ID) AS has_non_null,
           ROW_NUMBER() OVER (
               PARTITION BY PHARM_CLM_ID
               ORDER BY CASE WHEN TRANS_TYP_CD IS NOT NULL THEN 1 ELSE 0 END DESC
           ) AS rn
    FROM your_table_name t
) sub
WHERE rn = 1   -- pick only one best row per claim
ORDER BY PHARM_CLM_ID;
