SELECT
  CASE_ID,
  CASE_BGINDT,
  CASE_END_DT,
  DATEDIFF(DAY, CASE_BGINDT, CASE_END_DT) AS CASE_DURATION_DAYS,
  CASE_STS_CD,
  CASE_CLNDR_DAY_CNT,
  CASE_BUS_BSC_DAY_CNT,
  CASE_BUS_FED_DAY_CNT,
  clndr_dt,
  BSC_HOL_IND,
  FED_HOL_IND,
  WKDAY_IND,
  WKEND_IND
FROM V_SMNTC_CUST_SVC_CASE b
LEFT JOIN EC_CXP_IM_STG1.TARGET.DAL.CLNDR_DIM a
  ON a.clndr_dt = b.case_bgindt AND a.bsc_hol_ind = 1
WHERE b.case_bgindt > '2024-01-01' 
  AND bsc_hol_ind IS NOT NULL
LIMIT 50;
